server {
    listen 80;
    server_name weather.matejc.com;

    access_log /var/log/nginx/weatherapp.access.log;
    error_log /var/log/nginx/weatherapp.error.log;

    location / {
        proxy_pass http://127.0.0.1:8000/;
    }

    location /api {
        proxy_pass http://127.0.0.1:9000;
    }
}
